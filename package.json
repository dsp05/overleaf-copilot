    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@types/crypto-js": "^4.1.2",
    "@types/diff": "^5.2.3",
    "@types/node": "^20.6.2",
    "crypto-js": "^4.1.1",
    "diff": "^7.0.0",
    "lucide": "^0.453.0",
    "lucide-preact": "^0.453.0",
